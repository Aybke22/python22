import math

# Öklid mesafesini hesaplayan fonksiyon

def euclideanDistance(point1, point2) :
     return math.sqrt((point2[0]- point[0]**2 + (point2[1])-point1[1])**2)

# 2D uzaydaki noktalar
points = [(1,2) , (4,6) , (5,8) , (2,1) ,(9,7)]

